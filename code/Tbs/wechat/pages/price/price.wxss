.pop {
    position: fixed;
    top: 50%;
    left: 50%;
    z-index: 1000;
    min-width: 400rpx;
    height: 80rpx;
    line-height: 80rpx;
    transform: translate(-50%, -50%);
    background-color: rgba(0, 0, 0, 0.5);
    text-align: center;
    color: #fff;
    font-size: 28rpx;
    border-radius: 4rpx;
    -webkit-transition: all 0.5s;
    transition: all 0.5s;
}
