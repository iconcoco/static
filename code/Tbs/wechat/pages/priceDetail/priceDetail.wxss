.pop {
    position: fixed;
    top: 50%;
    left: 50%;
    z-index: 1000;
    min-width: 400rpx;
    height: 80rpx;
    line-height: 80rpx;
    transform: translate(-50%, -50%);
    background-color: rgba(0, 0, 0, 0.5);
    text-align: center;
    color: #fff;
    font-size: 28rpx;
    border-radius: 4rpx;
    -webkit-transition: all 0.5s;
    transition: all 0.5s;
}

.pop-header {
    width: 100%;
}
.pop-header text {
    font-size: 36rpx;
    color: #222;
    line-height: 80rpx;
    text-align: center;
    margin-top: 20rpx;
    display: block;
    padding: 0;
}
.pop-ban {
    width: 100%;
    height: 200rpx;
    display: -webkit-box;
    display: box;
    -webkit-box-orient: vertical;
    box-orient: vertical;
    -webkit-box-align: center;
    box-align: center;
    -webkit-box-pack: center;
    box-pack: center;
}
.pop-ban text {
    display: block;
    font-size: 28rpx;
    color: #666;
    text-align: center;
    line-height: 1.3;
}
.pop-footer {
    width: 100%;
    height: 100rpx;
}
.pop-footer text {
    display: block;
    width: 360rpx;
    background-color: #FF6F20;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    height: 70rpx;
    line-height: 70rpx;
    border-radius: 500rpx;
    -webkit-border-radius: 500rpx;
    margin: 0 auto;
}
.tbs-footer {
  width: 100%;
  height: auto;
  text-align: center;
  margin: 20rpx 0 0;
  padding-bottom: 16rpx;
}
.tbs-footer text {
  display: block;
}
.tbs-footer text:nth-child(1) {
  color: #555;
  font-size: 24rpx;
  padding-bottom: 12rpx;
  line-height: 1;
}
.tbs-footer text:nth-child(2) {
  color: #979696;
  font-size: 18rpx;
  line-height: 1;
}